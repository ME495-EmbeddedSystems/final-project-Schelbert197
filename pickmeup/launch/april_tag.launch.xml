      </include>


    <node pkg="apriltag_ros" exec="apriltag_node" name="apriltag1">
        <remap from="image_rect" to="camera/color/image_raw"/>
        <remap from="camera_info" to="camera/color/camera_info"/>
        
       <param name="params-file" value="$(find-pkg-share pickmeup)/tags1.yaml"/>
       

    </node>
